<h1>login</h1>
<p id="notice"><%=notice%></p>

<%=form_tag do%>
	<table>
		<tr>
			<td>First Name:</td>
			<td><%=text_field_tag :firstName%></td>
		</tr>
		<tr>
			<td>Password:</td>
			<td><%=password_field_tag :password%></td>
		</tr>
		<tr>
			<td><%=submit_tag "Login" %></td>
		</tr>
	</table>
<%end%>
